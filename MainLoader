-- MainLoader
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local currentGameId = game.PlaceId
local gameName = game:GetService("MarketplaceService"):GetProductInfo(currentGameId).Name

local function Notify(title, text, duration)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration or 5,
    })
end

setclipboard("https://discord.gg/UKYHGddQD3")

Notify("Thanks For Choosin Us", "Every Script u load use the same key", 3)

Notify("Discord Link Copied", "Join us", 3)

local GameScripts = {
    -- The Forge [BETA] 1 world
    [76558904092080] = "https://v0-lua-script-hosting.vercel.app/api/script/78f2ef82-0554-4674-a0ff-a96d4d7f304e",

    -- The Forge [BETA] 2 world
    [129009554587176] = "https://v0-lua-script-hosting.vercel.app/api/script/78f2ef82-0554-4674-a0ff-a96d4d7f304e",
    
    -- Fisch
    [16732694052] = "https://v0-lua-script-hosting.vercel.app/api/script/d2b133cc-0293-44ac-87e3-9bfeb916e8e9",

    -- Fisch [New]
    [131716211654599] = "https://v0-lua-script-hosting.vercel.app/api/script/d2b133cc-0293-44ac-87e3-9bfeb916e8e9",
    
    -- Plants Vs Brainrots
    [127742093697776] = "https://v0-lua-script-hosting.vercel.app/api/script/5b76aadd-4acd-4975-8da1-5b10f81bb659",

    -- [FPS] Flick
    [136801880565837] = "https://v0-lua-script-hosting.vercel.app/api/script/9809883c-7d75-419e-a5a6-54c83424ac17",

    -- Blind Shoot
    [118614517739521] = "https://v0-lua-script-hosting.vercel.app/api/script/540d7403-3dc7-4f18-aea2-5574498a97cf",

    -- Fish It!
    [121864768012064] = "https://v0-lua-script-hosting.vercel.app/api/script/55fc230e-7821-446c-8084-37b7b3748392",
    
    
}

print("üìå Game ID:", currentGameId)
print("üéÆ Game Name:", gameName)

if GameScripts[currentGameId] then
    Notify("‚úÖ Game Detected!", gameName, 3)
    wait(1)
    
    Notify("üì¶ Loading Script...", "Please wait...", 3)
    
    local success, result = pcall(function()
        loadstring(game:HttpGet(GameScripts[currentGameId]))()
    end)
    
    if success then
        Notify("‚úÖ Script Loaded!", "Enjoy!", 5)
        print("‚úÖ Script loaded successfully for:", gameName)
    else
        Notify("‚ùå Load Failed!", "Check console (F9)", 5)
        warn("‚ùå Error loading script:", result)
    end
else
    Notify("‚ö†Ô∏è Game Not Supported", "Loading universal script...", 3)
    print("‚ö†Ô∏è No specific script for game ID:", currentGameId)
    wait(1)
    
    -- Load universal script as fallback for no need brooo
    local success, result = pcall(function()
        loadstring(game:HttpGet(UniversalScript))()
    end)
    
    if success then
        Notify("‚úÖ Universal Script Loaded!", "Limited features available", 5)
        print("‚úÖ Universal script loaded")
    else
        Notify("‚ùå All Scripts Failed!", "Contact Support Using Discord Link", 5)
        warn("‚ùå Error loading universal script:", result)
    end
end
