-- MainLoader
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

local currentGameId = game.PlaceId
local gameName = game:GetService("MarketplaceService"):GetProductInfo(currentGameId).Name

local function Notify(title, text, duration)
    StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration or 5,
    })
end

setclipboard("https://discord.gg/UKYHGddQD3")

Notify("Thanks For Choosin Us", "Every Script u load use the same key", 3)

Notify("Discord Link Copied", "Join us", 3)

local GameScripts = {
