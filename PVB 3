                return
            end
            
            WindUI:Notify({
                Title = "Gear Auto Buy Started",
                Content = "Buying " .. #SELECTED_GEAR .. " gear item(s) continuously",
                Duration = 3,
                Icon = "play",
            })
            startGearAutoBuy()
        else
            WindUI:Notify({
                Title = "Gear Auto Buy Stopped",
                Content = "Gear auto-buy has been disabled",
                Duration = 2,
                Icon = "stop-circle",
            })
        end
    end
})

